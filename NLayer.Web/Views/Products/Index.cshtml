
@{
    ViewData["Title"] = "Index";
}
@model List<ProductWithCategoryDto>
<table class="table table-striped">
    <thread>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Price</td>
            <td>Stock</td>
            <td>Category</td>
            <td>CreatedDate</td>
        </tr>
    </thread>
    <tbody>
        @foreach(var item in model)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Price</td>
            <td>@item.Stock</td>
            <td>@item.Category</td>
            <td>@item.CreatedDate</td>
        </tr>
        }
    </tbody>
</table>

